{
  "version": 2,
  "builds": [
    { "src": "**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/pdf/(.*)", "dest": "/pdf/$1" },
    { "src": "/pdf materi/(.*)", "dest": "/pdf materi/$1" },
    { "src": "/pdf jobsheet/(.*)", "dest": "/pdf jobsheet/$1" },
    { "src": "/materi", "dest": "/materi.html" },
    { "src": "/quiz", "dest": "/quiz.html" },
    { "src": "/simulasi", "dest": "/simulasi.html" },
    { "src": "/jobsheet", "dest": "/jobsheet.html" },
    { "src": "/tentang", "dest": "/tentang.html" },
    { "src": "/(.*)\\.html", "dest": "/$1.html" },
    { "src": "/(.*)\\.css", "dest": "/$1.css" },
    { "src": "/(.*)\\.js", "dest": "/$1.js" },
    { "src": "/", "dest": "/index.html" }
  ]
}